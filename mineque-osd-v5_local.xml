<xiroot>

    <osd enabled="yes" type="image" x="0"   y="0"   src="./images/v5.2-bg.png" />

    <!-- GPS Strenght visualizer. Need to solve img swapping -->

    <osd enabled="no"  type="button" x="0" y="0" command="play()"   src="dark/appbar.connection.quality.extremelylow.png" />
    <osd enabled="no"  type="button" x="0" y="0" command="play()"   src="dark/appbar.connection.quality.verylow.png" />
    <osd enabled="no"  type="button" x="0" y="0" command="play()"   src="dark/appbar.connection.quality.low.png" />
    <osd enabled="no"  type="button" x="0" y="0" command="play()"   src="dark/appbar.connection.quality.medium.png" />
    <osd enabled="no"  type="button" x="0" y="0" command="play()"   src="dark/appbar.connection.quality.high.png" />
    <osd enabled="yes" type="button" x="0" y="0" command="play()"   src="dark/appbar.connection.quality.veryhigh.png" />

    <!-- Data Connection quality -->
    <osd enabled="no" type="button" x="30" y="0" command="play()"   src="dark/appbar.connection.edge.png" /> <!-- non-existing -->
    <osd enabled="yes" type="button" x="50" y="0" command="play()"   src="dark/appbar.connection.3g.png" />
    <osd enabled="no" type="button" x="30" y="0" command="play()"   src="dark/appbar.connection.4g.png" />

    <!-- WIFI status? maybe create white/gray or green/red versions?-->
    <osd enabled="yes" type="button" x="110" y="0" command="play()"   src="dark/appbar.connection.wifi.png" />


    <osd enabled="yes" type="button" x="720" y="0" command="gui.quit()" src="dark/appbar.futurama.bender.png" />

    <osd x="40" y="0" w="560" h="40" align="4" type="text" label="${navigation.item.street_name_systematic} ${navigation.item.street_name}" font_size="300" background_color="#00000000" />
    <osd x="670" y="60" w="120" h="120" type="navigation_next_turn" background_color="#00000000" icon_src="%s_wh_96_96.png" />


    <!--variant 1 zooming above-->

    <osd enabled="yes" type="text"    x="-90" y="225" w="90" h="60" background_color="#000000cc" label=" "/>

    <osd enabled="yes" type="button"  x="-90" y="225" w="90" h="60" command="zoom_in()" src=""/>

    <osd enabled="yes" type="text"    x="-80" y="285" w="80" h="70" background_color="#ffffffca" label=" "/>
    <osd enabled="yes" type="button"  x="-80" y="285" w="80" h="70" command="zoom_to_route()" src=""/>

    <osd enabled="yes" type="text"    x="-70" y="355" w="70" h="65" background_color="#ddddddca" label=" "/>
    <osd enabled="yes" type="button"  x="-70" y="355" w="70" h="65" command="pitch=pitch==0?20:0;" src=""/>

    <osd enabled="no" type="text"    x="-65" y="420" w="65" h="60" background_color="#000000cc" label=" "/>
    <osd enabled="yes" type="button"  x="-65" y="420" w="65" h="60" command="zoom_out()" src="" />


    <osd enabled="yes" type="text"   x="0"   y="400" w="50" h="80" background_color="#ddddddca" label=" "/>
    <osd enabled="yes" type="button" x="0"   y="400" w="50" h="80" src="" command='spawn("amixer", "set", "Headphone", "5%-")'/>

    <osd enabled="yes" type="text"   x="50"  y="410" w="69" h="70" background_color="#000000cc" label=" "/>
    <osd enabled="yes" type="button" x="50"  y="410" w="69" h="70" src="" command="gui.spotify_previous_track()"/>

    <osd enabled="yes" type="text"   x="119" y="415" w="70" h="65" background_color="#ddddddca" label=" "/>
    <osd enabled="yes" type="button" x="119" y="415" w="70" h="65" src="" command="gui.spotify_toggle()"/>

    <osd enabled="yes" type="text"   x="189" y="418" w="66" h="62" background_color="#000000cc" label=" "/>
    <osd enabled="yes" type="button" x="189" y="418" w="66" h="62" src="" command="gui.spotify_next_track()"/>

    <osd enabled="yes" type="text"   x="255" y="420" w="70" h="60" background_color="#ddddddca" label=" "/>
    <osd enabled="yes" type="button" x="255" y="420" w="70" h="60" src="" command='spawn("amixer", "set", "Headphone", "5%+")'/>



    <osd enabled="yes" type="button" x="0" y="-90" command="play()"   src="dark/appbar.sound.mute.png" />

    <osd enabled="no" type="button" x="50"  y="-90" command="play()"   src="dark/appbar.sound.1.png" />

    <osd enabled="no" type="button" x="100"   y="-90" command="play()"   src="dark/appbar.control.rewind.variant.png" />

    <osd enabled="no" type="button" x="150"  y="-90" command="play()"   src="dark/appbar.control.play.png" />

    <osd enabled="no" type="button" x="200" y="-90" command="play()"   src="dark/appbar.control.pause.png" />

    <osd enabled="no" type="button" x="250" y="-90" command="play()"   src="dark/appbar.control.fastforward.variant.png" />

    <osd enabled="no" type="button" x="-76"  y="-90" command="play()"   src="dark/appbar.sound.3.png" />

    <osd enabled="no" type="button" x="-160" y="-90" command="orientation=orientation==0?-1:0;pitch=pitch==0?15:0;zoom=16;pitch==0?zoom_to_route()"   src="dark/appbar.globe.png" />

